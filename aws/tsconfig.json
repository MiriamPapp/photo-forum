{
  "include": [
      "./**/*"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ],
  "files": [
    "deploy.ts",
  ],
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "strict": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es6",
  }
}
